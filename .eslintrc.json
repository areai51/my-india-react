{
  "extends": "airbnb",
  "rules": {
    "max-len": [
      "warn",
      150
    ],
    "comma-dangle": 0,
    "react/prefer-es6-class": 0,
    "react/no-multi-comp": 0,
    "react/jsx-filename-extension": 0,
    "new-cap": 0,
    "no-return-assign": 0,
    "no-use-before-define": 0,
    "no-undef": 0,
    "prefer-template": 0,
    "no-param-reassign": 1,
    "no-underscore-dangle": 0,
    "react/jsx-no-bind": 0
  },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
